                            {
                              "name": "ScaleIn1",
                              "type": "Microsoft.Common.TextBox",
                              "defaultValue":"60",
                              "label": "CPU threshold (%)"
                            },
                            {
                              "name": "ScaleIn2",
                              "type": "Microsoft.Common.TextBox",
                              "defaultValue":"60",
                              "label": "Inbound traffic (Mbps)"
                            },
                            {
                                "name": "ScaleIn3",
                                "type": "Microsoft.Common.TextBox",
                                "defaultValue":"1",
                                "label": "Number of VMS to increase by"
                              }                            
                        ],
                        "visible":"[equals(steps('ScalesetSettings').EnaDisScalesetSettings, 'Enable')]"           
