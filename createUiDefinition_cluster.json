                                "label":"Real 10 IP Address",
                                "defaultValue":"",
                                "constraints":{  
                                    "required":true,
                                    "regex":"^(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)$",
                                    "validationMessage":"Format should be a valid IPv4 address"
                                },
                                "visible":"[greater(steps('SLBSetting').RealsCount, 9)]"
                            }
                        ]
                    },
                    {  
                        "name":"SLBPortNumber",
                        "type":"Microsoft.Common.TextBox",
                        "label":"Service Port Number",
                        "defaultValue":"80",
                        "constraints":{  
                            "regex":"^\\d+$",
                            "validationMessage":"Format should be valid tcp port"
                        },
                        "toolTip":"The port number of the service you want to load balance"
                    },
                    {  
                        "name":"SLBMetric",
                        "type":"Microsoft.Common.DropDown",
                        "label":"Metrics",
                        "defaultValue":"Least connections",
                        "toolTip":"The load balancing metrics you want to use",
                        "constraints":{  
                            "allowedValues":[  
                                {  
                                    "label":"Least connections",
                                    "value":"leastconns"
                                },
                                {  
                                    "label":"Round robin",
                                    "value":"roundrobin"
                                },
                                {  
                                    "label":"Response time",
                                    "value":"response"
                                }
                            ]
                        }
                    }                    
                ]
            }                                                                        
        ],
        "outputs":{  
            "location":"[location()]",
            "VMPrefixName":"[basics('AlteonClusterName')]",
            "adminUsername":"[if(equals(steps('VmssInstanceCredentials').EnaDisVmssInstanceCredentials, 'Enable'), steps('VmssInstanceCredentials').adminUsername, '')]",
            "adminPassword":"[if(equals(steps('VmssInstanceCredentials').EnaDisVmssInstanceCredentials, 'Enable'), steps('VmssInstanceCredentials').adminPassword, '')]",
            "CloudLicenseServerDeviceId":"[if(equals(steps('GelSettings').ClsLlsGelSettings, 'Cloud') ,steps('GelSettings').CloudLicenseServerDeviceId, '')]",
            "PrimaryDnsIP":"[if(equals(steps('GelSettings').ClsLlsGelSettings, 'Cloud') ,steps('GelSettings').PrimaryDnsIP, '')]",
            "SecondaryDnsIP":"[if(equals(steps('GelSettings').ClsLlsGelSettings, 'Cloud') ,steps('GelSettings').SecondaryDnsIP, '')]",
            "PrimaryLlsIP":"[if(equals(steps('GelSettings').ClsLlsGelSettings, 'Local') ,steps('GelSettings').PrimaryLlsIP, '')]",
            "SecondaryLlsIP":"[if(equals(steps('GelSettings').ClsLlsGelSettings, 'Local') ,steps('GelSettings').SecondaryLlsIP, '')]",     

            "StorageAccountName":"[steps('infrastructureConfig').storageAccount.name]",
            "vmSize":"[steps('infrastructureConfig').vmSize]",
            "vmCount":"[steps('infrastructureConfig').VmCount]",
            "dnsNameForPublicIP":"[steps('infrastructureConfig').dnsAndPublicIP.domainNameLabel]",
            "ClientID":"[steps('ActiveDirSetting').ActiveDirectoryCredentials.ClientID]",
            "ClientSecret":"[steps('ActiveDirSetting').ActiveDirectoryCredentials.ClientSecret]",
            "TenantID":"[steps('ActiveDirSetting').ActiveDirectoryCredentials.TenantID]",
            "DNSServerIP":"[steps('ActiveDirSetting').DNSServerIP]",
            "RealsCount":"[string(steps('SLBSetting').RealsCount)]",
            "SLBPortNumber":"[steps('SLBSetting').SLBPortNumber]",
            "SLBMetric":"[steps('SLBSetting').SLBMetric]"
        }
    }
